#ifndef MAIN_H
#define MAIN_H

/**
 * _putchar - Entry point of the program
 *
 * Description: Prints a quote using the puts function
 * Return: Always 0 (Success)
 */

int _putchar(char c);

void print_alphabet(void)
{
char alphabet = 'a';

while (alphabet <= 'z')
{
_putchar(alphabet);

alphabet++;
}
_putchar('\n');
}

#endif
